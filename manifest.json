{
  "manifest_version": 2,

  "name": "Download Mp3",
  "description": "This extension downloads mp3 files from youtube",
  "icons":
   {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}